// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id Int @id @unique @default(autoincrement())
  firstname String 
  lastname String
  email String @unique
  imageURL String
  gpa Int
}
